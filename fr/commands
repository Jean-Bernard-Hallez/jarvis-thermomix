>(*)==if [[ "$GOTOSORTIMOMO" == "Fin" ]]; then GOTOSORTIMOMO=""; commands="$(jv_get_commands)"; jv_handle_order "GOTOSORTIMOMO"; return; else REPONSEMOMO="$order"; commands="$(jv_get_commands)"; jv_handle_order "Recette Thermomix"; fi; return
